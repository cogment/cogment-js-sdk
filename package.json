{
  "name": "@cogment/cogment-js-sdk",
  "version": "1.24.0",
  "description": "Javascript SDK for the Cogment Framework",
  "homepage": "https://github.com/cogment/cogment-js-sdk",
  "bugs": "https://github.com/cogment/cogment-js-sdk/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogment/cogment-js-sdk.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Artificial Intelligence Redefined <dev+cogment@ai-r.com>",
  "main": "dist/cogment.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "doc": "docs",
    "example": "examples",
    "test": "__tests__"
  },
  "files": [
    "dist/index.{d.ts,t.ts.map}",
    "dist/cogment.{js,js.mapi,js.LICENSE.txt}",
    "dist/cogment.umd.{js,js.map,js.LICENSE.txt}",
    "dist/cogment.esm.{js,js.map,js.LICENSE.txt}"
  ],
  "scripts": {
    "test": "npm run test:jest",
    "analyze": "npx size-limit --why",
    "build": "npx npm-run-all build:clean build:protos build:webpack build:typings",
    "build:clean": "npx rimraf dist public cogment cogment-api.tar.gz cogment-api-* src/cogment/api",
    "build:docs": "typedoc --plugin typedoc-plugin-markdown",
    "build:install": "npm install",
    "build:protos": "npx npm-run-all build:protos:fetch build:protos:compile",
    "build:protos:compile": "protoc -I . cogment/api/*.proto --js_out=import_style=commonjs,binary:src --ts_out=service=grpc-web:src",
    "build:protos:fetch": "cogjs-cli fetch-protos",
    "build:tsc": "tsc",
    "build:typings": "npm run build:tsc -- --declaration --declarationMap --emitDeclarationOnly --outFile dist/index.d.ts",
    "build:webpack": "webpack",
    "build:webpack:watch": "npm run build:webpack -- --watch",
    "cz": "git-cz",
    "fiddly": "npx fiddly",
    "init": "npm install && npx npm-run-all init:*",
    "init:0": "npx npm-run-all --parallel init:0:*",
    "init:0:install": "cd cli && npm install && npm install -g .",
    "init:0:install-cli": "cd cli && npm install && npm install -g .",
    "init:0:install-test-suite": "cd __tests__/end-to-end/cogment-app/webapp && npm install",
    "init:1": "npx npm-run-all --parallel init:1:*",
    "init:1:docker-compose-overrride": "cogjs-cli init:docker-compose-override",
    "init:1:fetch-cogment-cli": "cogjs-cli fetch-cogment-cli --out bin/cogment",
    "init:2": "npx npm-run-all --parallel init:2:*",
    "init:2:generate": "export PATH=$PWD/bin:$PATH; cd ./__tests__/end-to-end/cogment-app/ && cogment run generate",
    "init:2:protos": "npm run build:protos",
    "lint": "npx npm-run-all lint:{eslint,prettier,remark}:check",
    "lint:eslint": "eslint .",
    "lint:eslint:check": "npm run lint:eslint",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npx npm-run-all lint:{prettier,eslint}:fix",
    "lint:prettier": "prettier .",
    "lint:prettier:check": "npm run lint:prettier -- --check",
    "lint:prettier:fix": "npm run lint:prettier -- --write",
    "lint:remark": "remark .",
    "lint:remark:check": "npm run lint:remark .",
    "lint:remark:fix": "npm run lint:remark -- --fix",
    "migrate": "npx npm-run-all migrate:*",
    "migrate:conduct": "npx covgen 'dev+cogment@ai-r.com'",
    "report:legal": "npx npm-run-all report:legal:*",
    "report:legal:legally": "npx legally -lr",
    "report:legal:tldrlegal": "npx tldrlegal",
    "semantic-release": "npx semantic-release",
    "size": "npx size-limit",
    "start": "webpack serve",
    "storybook": "npx storybook",
    "test:ci": "npx npm-run-all test:{jest,reports}",
    "test:jest": "jest --runInBand",
    "test:reports": "npx npm-run-all --parallel test:reports:*",
    "test:reports:allure": "allure generate --clean",
    "test:ui": "npx majestic --noOpen",
    "test:watch": "npm run test:jest -- --watch"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.14.0",
    "debug": "^4.3.1",
    "typedoc-plugin-markdown": "^3.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@microsoft/api-extractor": "^7.13.2",
    "@microsoft/tsdoc": "^0.13.0",
    "@microsoft/tsdoc-config": "^0.14.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.5",
    "@types/decompress": "^4.2.3",
    "@types/eslint": "^7.2.7",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/long": "^4.0.1",
    "@types/node": "^14.14.35",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@webpack-cli/init": "^1.1.3",
    "@webpack-cli/migrate": "^1.1.2",
    "axios": "^0.21.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "cosmiconfig": "^7.0.0",
    "cross-spawn": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "decompress": "^4.2.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-plugin-unicorn": "^29.0.0",
    "eslint-webpack-plugin": "^2.5.3",
    "execa": "^5.0.0",
    "fs-jetpack": "^4.1.0",
    "google-protobuf": "^3.15.6",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jest-allure": "^0.1.3",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "jest-junit": "^12.0.0",
    "jest-sonar": "^0.2.12",
    "jest-watch-typeahead": "^0.6.1",
    "json5": "^2.2.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "npm-merge-driver": "^2.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "prettier-plugin-packagejson": "^2.2.10",
    "prettier-plugin-properties": "^0.1.0",
    "prettier-plugin-sh": "^0.6.0",
    "remark": "^13.0.0",
    "remark-cli": "^9.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-github": "^10.0.1",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "remark-toc": "^7.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "semver": "^7.3.5",
    "source-map-loader": "^2.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-protoc-gen": "^0.14.0",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.34",
    "typedoc-neo-theme": "^1.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.2"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest-junit": {
    "suiteName": "cogment",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": true,
    "classNameTemplate": "{classname} {title}",
    "titleTemplate": "{title}",
    "suiteNameTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
