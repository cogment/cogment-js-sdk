{
  "name": "cogment-js-sdk",
  "version": "1.0.0",
  "description": "Javascript SDK for the Cogment Framework",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com/ai-r/cogment-js-sdk-1.0.git"
  },
  "author": "Artificial Intelligence Redefined <dev+cogment@ai-r.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "npm run protos && npm run compile",
    "compile": "tsc",
    "docs": "typedoc src/**/*.ts",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint --cache {src,test}/**/*.ts",
    "lint:prettier": "prettier --check {src,test}/**/*.ts",
    "pretty-quick": "pretty-quick",
    "protos": "protoc -I node_modules/cogment-api node_modules/cogment-api/*.proto --js_out=import_style=commonjs,binary:src/protos --ts_out=service=grpc-web:src/protos",
    "size": "size-limit",
    "start": "node dist/index.js",
    "test": "jest test"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/eslint": "^7.2.5",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/long": "^4.0.1",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "cogment-api": "gitlab:ai-r/cogment-api#v0.4.1",
    "doctoc": "^1.4.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "google-protobuf": "^3.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.2",
    "np": "^7.0.0",
    "prettier": "^2.2.0",
    "prettier-plugin-pkg": "^0.8.0",
    "prettier-plugin-sh": "^0.6.0",
    "rimraf": "^3.0.2",
    "size-limit": "^4.9.0",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-protoc-gen": "^0.13.0",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{ts,js,css,md,json}": "prettier --write"
  },
  "size-limit": [
    {
      "path": "dist/cogment-js-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cogment-js-sdk.esm.js",
      "limit": "10 KB"
    }
  ]
}
