{
  "name": "cogment",
  "version": "1.0.0",
  "description": "Javascript SDK for the Cogment Framework",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com/ai-r/cogment-js-sdk-1.0.git"
  },
  "author": "Artificial Intelligence Redefined <dev+cogment@ai-r.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "main": "dist/cogment.js",
  "module": "dist/cogment.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "npm-run-all build:protos build:webpack",
    "build:protos": "protoc -I node_modules/cogment-api node_modules/cogment-api/cogment/api/*.proto --js_out=import_style=commonjs,binary:src --ts_out=service=grpc-web:src",
    "build:webpack": "webpack",
    "docs": "typedoc",
    "lint": "npm-run-all lint:prettier:check lint:eslint:check",
    "lint:eslint": "eslint .",
    "lint:eslint:check": "npm run lint:eslint",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npm-run-all lint:{prettier,eslint}:fix",
    "lint:prettier": "prettier .",
    "lint:prettier:check": "npm run lint:prettier -- --check",
    "lint:prettier:fix": "npm run lint:prettier -- --write",
    "size": "size-limit",
    "start": "webpack serve",
    "test": "jest",
    "test:ui": "majestic --app",
    "tsc": "tsc",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.12.0",
    "@microsoft/tsdoc": "^0.12.24",
    "@microsoft/tsdoc-config": "^0.13.9",
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/eslint": "^7.2.6",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "@types/long": "^4.0.1",
    "@types/node": "^14.14.10",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@webpack-cli/init": "^1.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "cogment-api": "gitlab:ai-r/cogment-api#v1.0.0-alpha2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-webpack-plugin": "^2.4.1",
    "execa": "^5.0.0",
    "google-protobuf": "^3.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "json5": "^2.1.3",
    "lint-staged": "^10.5.3",
    "majestic": "^1.8.0",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "prettier-plugin-pkg": "^0.8.0",
    "prettier-plugin-sh": "^0.6.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "size-limit": "^4.9.0",
    "source-map-loader": "^1.1.3",
    "terser-webpack-plugin": "^5.0.3",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-protoc-gen": "^0.13.0",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "browserslist": [
    "defaults"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,md,json,yaml,yml}": "npm run lint:prettier:check",
    "*.{ts,js,json}": "npm run lint:eslint",
    "*.ts": "npm run test"
  },
  "size-limit": [
    {
      "path": "dist/cogment-js-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cogment-js-sdk.esm.js",
      "limit": "10 KB"
    }
  ]
}
