{
  "name": "cogment",
  "version": "1.0.0",
  "description": "Javascript SDK for the Cogment Framework",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com/ai-r/cogment-js-sdk-1.0.git"
  },
  "author": "Artificial Intelligence Redefined <dev+cogment@ai-r.com>",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "main": "dist/main.bundle.js",
  "module": "",
  "types": "dist/index.d.ts",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "npm run protos && npm run webpack",
    "docs": "typedoc",
    "lint": "npm run lint:prettier:check && npm run lint:eslint",
    "lint:eslint": "eslint --cache {src,test}/**/*.ts",
    "lint:prettier": "prettier .",
    "lint:prettier:check": "npm run lint:prettier -- --check",
    "lint:prettier:write": "npm run lint:prettier -- --write",
    "protos": "protoc -I node_modules/cogment-api node_modules/cogment-api/cogment/api/*.proto --js_out=import_style=commonjs,binary:src --ts_out=service=grpc-web:src",
    "size": "size-limit",
    "start": "webpack serve",
    "test": "jest",
    "tsc": "tsc",
    "watch": "webpack --watch",
    "webpack": "webpack"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/eslint": "^7.2.6",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "@types/long": "^4.0.1",
    "@types/node": "^14.14.10",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@webpack-cli/init": "^1.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "cogment-api": "gitlab:ai-r/cogment-api#v1.0.0-alpha2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "google-protobuf": "^3.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.2",
    "np": "^7.0.0",
    "prettier": "^2.2.0",
    "prettier-plugin-pkg": "^0.8.0",
    "prettier-plugin-sh": "^0.6.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "size-limit": "^4.9.0",
    "source-map-loader": "^1.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-protoc-gen": "^0.13.0",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "npm run lint:eslint",
    "*.{ts,js,css,md,json,yaml,yml}": "npm run lint:prettier:check",
    "*.{ts}": "npm run test"
  },
  "size-limit": [
    {
      "path": "dist/cogment-js-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cogment-js-sdk.esm.js",
      "limit": "10 KB"
    }
  ]
}
