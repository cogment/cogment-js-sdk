{
  "name": "@cogment/cogment-js-sdk",
  "version": "1.10.2",
  "description": "Javascript SDK for the Cogment Framework",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:ai-r/cogment-js-sdk.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Artificial Intelligence Redefined <dev+cogment@ai-r.com>",
  "main": "dist/cogment.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "doc": "docs",
    "example": "examples",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "public",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "npx npm-run-all build:{clean,protos,typings,webpack}",
    "build:clean": "npx rimraf dist public cogment cogment-api.tar.gz cogment-api-* allure-{report,results} src/cogment",
    "build:docs": "typedoc",
    "build:install": "npm install",
    "build:protos": "npx npm-run-all build:protos:fetch build:protos:compile",
    "build:protos:compile": "protoc -I . cogment/api/*.proto --js_out=import_style=commonjs,binary:src --ts_out=service=grpc-web:src",
    "build:protos:fetch": "cogjs-cli fetch-protos",
    "build:tsc": "tsc",
    "build:typings": "npm run build:tsc -- --declaration --declarationMap --emitDeclarationOnly --outFile dist/index.d.ts",
    "build:webpack": "webpack",
    "build:webpack:watch": "npm run build:webpack -- --watch",
    "cli:install": "cd cli && npm install",
    "cz": "git-cz",
    "init": "npx npm-run-all build:install cli:install init:*",
    "init:docker-compose-overrride": "cogjs-cli init:docker-compose-override",
    "init:protos": "npm run build:protos",
    "lint": "npx npm-run-all lint:{prettier,eslint}:check",
    "lint:eslint": "eslint .",
    "lint:eslint:check": "npm run lint:eslint",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npx npm-run-all lint:{prettier,eslint}:fix",
    "lint:prettier": "prettier .",
    "lint:prettier:check": "npm run lint:prettier -- --check",
    "lint:prettier:fix": "npm run lint:prettier -- --write",
    "migrate": "npx npm-run-all migrate:*",
    "migrate:conduct": "npx covgen 'dev+cogment@ai-r.com'",
    "report:legal": "npx npm-run-all report:legal:*",
    "report:legal:legally": "npx legally -lr",
    "report:legal:tldrlegal": "npx tldrlegal",
    "semantic-release": "semantic-release",
    "size": "size-limit",
    "start": "webpack serve",
    "test": "npm run test:jest",
    "test:ci": "npx npm-run-all test:{jest,reports}",
    "test:jest": "jest",
    "test:reports": "npx npm-run-all --parallel test:reports:*",
    "test:reports:allure": "allure generate --clean",
    "test:reports:codecov": "codecov",
    "test:ui": "npx majestic --noOpen",
    "test:watch": "npm run test:jest -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && [ -z ${CI} ] && git cz --hook || true",
      "commit-msg": "[ -z ${CI} ] && npx commitlint --edit \"$1\" || true"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,md,json,yaml,yml}": "npm run lint:prettier:fix",
    "*.{ts,js,json}": "npm run lint:eslint:fix",
    "*!(.d).ts": "npm run test:jest -- --findRelatedTests --passWithNoTests"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "debug": "^4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@improbable-eng/grpc-web-node-http-transport": "^0.13.0",
    "@microsoft/api-extractor": "^7.13.0",
    "@microsoft/tsdoc": "^0.12.24",
    "@microsoft/tsdoc-config": "^0.13.9",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.8",
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.5",
    "@types/decompress": "^4.2.3",
    "@types/eslint": "^7.2.6",
    "@types/google-protobuf": "^3.7.4",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/long": "^4.0.1",
    "@types/node": "^14.14.21",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@webpack-cli/init": "^1.1.1",
    "@webpack-cli/migrate": "^1.1.2",
    "axios": "^0.21.1",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "cosmiconfig": "^7.0.0",
    "cross-spawn": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "decompress": "^4.2.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-tsdoc": "^0.2.11",
    "eslint-plugin-unicorn": "^26.0.1",
    "eslint-webpack-plugin": "^2.4.1",
    "execa": "^5.0.0",
    "fs-jetpack": "^4.1.0",
    "google-protobuf": "^3.14.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jest-allure": "^0.1.3",
    "jest-junit": "^12.0.0",
    "jest-sonar": "^0.2.11",
    "jest-watch-typeahead": "^0.6.1",
    "json5": "^2.1.3",
    "lint-staged": "^10.5.3",
    "npm-merge-driver": "^2.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "prettier-plugin-packagejson": "^2.2.9",
    "prettier-plugin-properties": "^0.1.0",
    "prettier-plugin-sh": "^0.6.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.2",
    "semver": "^7.3.4",
    "size-limit": "^4.9.0",
    "source-map-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-protoc-gen": "^0.14.0",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.14",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.2"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "cogment": {
    "connection": {
      "http": "http://grpcwebproxy:8080"
    },
    "logger": {
      "level": "debug"
    }
  },
  "cogment-api": {
    "cogment_api_version": "1.0.0-alpha4"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "prepare": true,
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/gitlab",
        {
          "assets": [
            {
              "path": "dist/cogment.js",
              "label": "CommonJS2 bundle"
            },
            {
              "path": "dist/cogment.esm.js",
              "label": "ESM bundle"
            },
            {
              "path": "dist/cogment.umd.js",
              "label": "UMD bundle"
            }
          ]
        }
      ],
      "@semantic-release/git"
    ]
  },
  "size-limit": [
    {
      "path": "dist/cogment.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/cogment.umd.js",
      "limit": "300 KB"
    }
  ]
}
