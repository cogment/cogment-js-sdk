{
  "name": "@cogment/cogment-js-cli",
  "version": "0.0.0",
  "description": "Command line tool for the cogment-js-sdk repository",
  "keywords": [
    "oclif",
    "cogment"
  ],
  "homepage": "https://github.com/cogment/cogment-js-sdk/",
  "bugs": "https://github.com/cogment/cogment-js-sdk/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogment/cogment-js-sdk.git",
    "directory": "cli"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Emma Coutu @ecoutu",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cogjs-cli": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "postinstall": "bin/run fetch-cogment-cli --out bin/cogment",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "test": "echo NO TESTS",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "axios": "^0.21.2",
    "cosmiconfig": "^7.0.0",
    "decompress": "^4.2.1",
    "follow-redirects": "^1.13.3",
    "fs-jetpack": "^4.1.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/decompress": "^4.2.3",
    "@types/follow-redirects": "^1.13.0",
    "@types/node": "^14.14.35",
    "eslint": "^7.22.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cogjs-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
