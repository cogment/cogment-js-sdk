{
  "name": "cogment_app",
  "version": "1.0.0",
  "description": "Bootstrap cogment_app Project",
  "author": "",
  "license": "ISC",
  "main": "main.js",
  "scripts": {
    "build": "npm-run-all build:clean build:install build:protos build:webpack",
    "build:clean": "rimraf node_modules dist public cogment cogment-api.tar.gz cogment-api-*",
    "build:install": "npm install",
    "build:protos": "protoc -I ../.. ../../*.proto --js_out=import_style=commonjs,binary:src --ts_out=service=grpc-web:src",
    "build:webpack": "webpack",
    "start": "webpack-dev-server --host 0.0.0.0 --hot --inline"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "google-protobuf": "^3.14.0",
    "ts-protoc-gen": "^0.13.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
