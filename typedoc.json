{
  "includeVersion": true,
  "entryPoints": [
    "src/ActorSession.ts",
    "src/Cogment.ts",
    "src/CogmentService.ts",
    "src/TrialController.ts",
    "src/lib/DeltaEncoding.ts",
    "src/lib/Logger.ts"
  ],
  "externalPattern": ["src/cogment/api/*"],
  "exclude": [
    "**/*.test.*",
    "node_modules/**/*",
    "__tests__/**/*",
    "src/__tests__",
    "src/__data__",
    "cogment",
    "cogment-api-*"
  ],
  "categorizeByGroup": true,
  "out": "docs/api"
}
